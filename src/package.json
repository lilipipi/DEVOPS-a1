{
  "name": "rmit-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=express-sequelize node ./bin/www",
    "start:prod": "cross-env NODE_ENV=production DEBUG=express-sequelize node ./bin/www",
    "test-lint": "eslint ./",
    "test-unit": "cross-env NODE_ENV=unit mocha test/unit --reporter mocha-junit-reporter --reporter-options mochaFile=./test_outputs/unit_test.xml",
    "test-coverage": "cross-env NODE_ENV=unit nyc --reporter=lcov --reporter=text-summary mocha test/unit",
    "test-integration": "cross-env DB_USERNAME=postgres DB_PASSWORD=password DB_NAME=servian DB_HOSTNAME=localhost NODE_ENV=integration ",
    "test-e2e": "cross-env NODE_ENV=e2e DB_USERNAME=postgres DB_PASSWORD=password DB_NAME=servian"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "pug": "^2.0.0-rc.4",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.1.0",
    "serve-favicon": "~2.3.0",
    "sqlite3": "^4.1.1",
    "qawolf": "^0.9.2"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^11.0.0",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "mocha-junit-reporter": "^1.23.3",
    "nyc": "^15.0.0",
    "supertest": "^2.0.0"
  }
}
